<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
		<FAMILY>Databases/DB Specifics/Amazon/Oracle</FAMILY>
    <FAMILY>Cloud/Amazon/Oracle</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

    <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:ORACLE"
	  NOT_SHOW_IF="USE_TNS_FILE == 'true'"
    />

    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
      REPOSITORY_VALUE="CONNECTION_TYPE"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
      </ITEMS>
    </PARAMETER>
	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25"
		REPOSITORY_VALUE="DB_VERSION">
		<ITEMS DEFAULT="ORACLE_18">
            <ITEM NAME="ORACLE_18" VALUE="ORACLE_18" />
			<ITEM NAME="ORACLE_12" VALUE="ORACLE_12" />
			<ITEM NAME="ORACLE_11" VALUE="ORACLE_11" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="USE_TNS_FILE" FIELD="CHECK"
		NUM_ROW="28"
		SHOW="true"
		>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="TNS_FILE"
      FIELD="FILE"
      NUM_ROW="29"
      SHOW_IF="USE_TNS_FILE == 'true'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="TNS_EDITOR"
	  FIELD="TNS_EDITOR"
      NUM_ROW="29"
	  SHOW_IF="USE_TNS_FILE == 'true'"
    >
    </PARAMETER>


    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Oracle</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET')"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCAL_SERVICE_NAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="CONNECTION_TYPE == 'ORACLE_OCI'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SCHEMA"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="50"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="80"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="85"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="NOTE"
		FIELD="LABEL"
		NUM_ROW="86"
		NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'">
			<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>

	</PARAMETER>

	<PARAMETER
      NAME="USE_SHARED_CONNECTION"
	  FIELD="CHECK"
      NUM_ROW="90"
      REQUIRED="true"
    >
	<DEFAULT>false</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="SHARED_CONNECTION_NAME"
	  FIELD="TEXT"
      NUM_ROW="90"
      REQUIRED="true"
	  SHOW_IF="USE_SHARED_CONNECTION == 'true'"
    >
	<DEFAULT></DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="JDBC_URL"
	  FIELD="TEXT"
      NUM_ROW="45"
      REQUIRED="true"
	  SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
	<DEFAULT>"jdbc:oracle:thin:USER/MDP@server"</DEFAULT>
	   </PARAMETER>
   </PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="AUTO_COMMIT" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
    	</PARAMETER>
	</ADVANCED_PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="TNS" MODULE="tns.jar" MVN="mvn:org.talend.libraries/tns/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="ORACLE_18"     MODULE="ojdbc8-19.3.0.0.jar" MVN="mvn:com.oracle.ojdbc/ojdbc8/19.3.0.0" REQUIRED_IF="(DB_VERSION == 'ORACLE_18')" />
       <IMPORT NAME="Driver-Ojdbc7" MODULE="ojdbc7.jar" MVN="mvn:org.talend.libraries/ojdbc7/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_12')" />
       <IMPORT NAME="Driver-Ojdbc6" MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_11')" />
       <IMPORT NAME="slf4j-api-1.7.34.jar" MODULE="slf4j-api-1.7.34.jar" MVN="mvn:org.slf4j/slf4j-api/1.7.34" REQUIRED_IF="USE_SHARED_CONNECTION =='true'" />
       <IMPORT NAME="slf4j-reload4j-1.7.34.jar" MODULE="slf4j-reload4j-1.7.34.jar" MVN="mvn:org.slf4j/slf4j-reload4j/1.7.34" REQUIRED_IF="USE_SHARED_CONNECTION =='true'" />
    </IMPORTS>
   </CODEGENERATION>

   <RETURNS/>
</COMPONENT>
